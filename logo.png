  </span><span><span class="hljs-variable constant_">URL</span></span><span>.</span><span><span class="hljs-title function_">revokeObjectURL</span></span><span>(url);
}

</span><span><span class="hljs-comment">// ---------- Export to PDF ----------</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">downloadPDF</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
  </span><span><span class="hljs-keyword">const</span></span><span> table = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"boq-table"</span></span><span>).</span><span><span class="hljs-property">outerHTML</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> total = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"totalAmount"</span></span><span>).</span><span><span class="hljs-property">textContent</span></span><span>;

  </span><span><span class="hljs-keyword">const</span></span><span> win = </span><span><span class="hljs-variable language_">window</span></span><span>.</span><span><span class="hljs-title function_">open</span></span><span>(</span><span><span class="hljs-string">""</span></span><span>, </span><span><span class="hljs-string">""</span></span><span>, </span><span><span class="hljs-string">"height=700,width=900"</span></span><span>);
