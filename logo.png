  </span><span><span class="hljs-keyword">const</span></span><span> url = </span><span><span class="hljs-variable constant_">URL</span></span><span>.</span><span><span class="hljs-title function_">createObjectURL</span></span><span>(blob);
  </span><span><span class="hljs-keyword">const</span></span><span> a = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"a"</span></span><span>);

