  win.</span><span><span class="hljs-property">document</span></span><span>.</span><span><span class="hljs-title function_">write</span></span><span>(</span><span><span class="hljs-string">"&lt;h2&gt;BOQ Estimate&lt;/h2&gt;"</span></span><span>);
  win.</span><span><span class="hljs-property">document</span></span><span>.</span><span><span class="hljs-title function_">write</span></span><span>(table);
  win.</span><span><span class="hljs-property">document</span></span><span>.</span><span><span class="hljs-title function_">write</span></span><span>(</span><span><span class="hljs-string">`&lt;h3&gt;Total: ₦<span class="hljs-subst">${total}</span></span></span><span>&lt;/h3&gt;`);
  win.</span><span><span class="hljs-property">document</span></span><span>.</span><span><span class="hljs-title function_">write</span></span><span>(</span><span><span class="hljs-string">"&lt;/body&gt;&lt;/html&gt;"</span></span><span>);
  win.</span><span><span class="hljs-property">document</span></span><span>.</span><span><span class="hljs-title function_">close</span></span><span>();
