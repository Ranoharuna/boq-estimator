</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">addRow</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
  </span><span><span class="hljs-keyword">const</span></span><span> itemName = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"itemName"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> unit = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"unit"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> quantity = </span><span><span class="hljs-built_in">parseFloat</span></span><span>(</span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"quantity"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>);
  </span><span><span class="hljs-keyword">const</span></span><span> rate = </span><span><span class="hljs-built_in">parseFloat</span></span><span>(</span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"rate"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>);

  </span><span><span class="hljs-keyword">if</span></span><span> (!itemName || !unit || </span><span><span class="hljs-built_in">isNaN</span></span><span>(quantity) || </span><span><span class="hljs-built_in">isNaN</span></span><span>(rate)) {
    </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"Please fill in all fields correctly."</span></span><span>);
    </span><span><span class="hljs-keyword">return</span></span><span>;
  }

  </span><span><span class="hljs-keyword">const</span></span><span> amount = quantity * rate;
  </span><span><span class="hljs-keyword">const</span></span><span> table = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"boq-body"</span></span><span>);

  </span><span><span class="hljs-keyword">const</span></span><span> row = table.</span><span><span class="hljs-title function_">insertRow</span></span><span>();
  row.</span><span><span class="hljs-property">innerHTML</span></span><span> = <span class="hljs-string">`
