</span><span><span class="hljs-comment">// ---------- Save to Local Storage ----------</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">saveDataToLocalStorage</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
  </span><span><span class="hljs-keyword">const</span></span><span> rows = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">querySelectorAll</span></span><span>(</span><span><span class="hljs-string">"#boq-body tr"</span></span><span>);
  </span><span><span class="hljs-keyword">let</span></span><span> data = [];

  rows.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">row</span></span></span><span>) =&gt; {
    </span><span><span class="hljs-keyword">let</span></span><span> rowData = {
      </span><span><span class="hljs-attr">itemName</span></span><span>: row.</span><span><span class="hljs-property">cells</span></span><span>[</span><span><span class="hljs-number">0</span></span><span>].</span><span><span class="hljs-property">textContent</span></span><span>,
      </span><span><span class="hljs-attr">unit</span></span><span>: row.</span><span><span class="hljs-property">cells</span></span><span>[</span><span><span class="hljs-number">1</span></span><span>].</span><span><span class="hljs-property">textContent</span></span><span>,
      </span><span><span class="hljs-attr">quantity</span></span><span>: row.</span><span><span class="hljs-property">cells</span></span><span>[</span><span><span class="hljs-number">2</span></span><span>].</span><span><span class="hljs-property">textContent</span></span><span>,
